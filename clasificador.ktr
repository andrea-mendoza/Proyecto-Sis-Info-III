    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import java.util.Iterator;
import java.util.Map;
import org.json.simple.JSONArray;
import org.json.simple.JSONObject;
import org.json.simple.parser.JSONParser;
import org.json.simple.parser.ParseException;
import java.io.IOException;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException,ParseException,IOException {
    Object[] inputRow = getRow(); //get the next input row

    if (inputRow == null) {
        setOutputDone(); //no more rows to process
        return false;
    }

    if (first) {
        //row processing init section.
        //make sure you set the flag to false when init has completed
        first = false;
    }

    // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.

   	Object[] outputRow = createOutputRow(inputRow, data.outputRowMeta.size()); //init the values using the input record
	String jsonResponse = get(Fields.Out, "jsonResponse").getString(outputRow);
	logBasic("* jsonResponse *" + jsonResponse.toString() );

	JSONParser parser = new JSONParser();	

	JSONObject jsonObject = (JSONObject) parser.parse(jsonResponse);

	JSONObject value;

	JSONArray comments;
	JSONArray results;

	String comment;
	Long result;
	String label;

   	Object[] output = createOutputRow(inputRow, data.outputRowMeta.size()); //init the values using the input record

	comments = (JSONArray) jsonObject.get("comments");
	results = (JSONArray) jsonObject.get("results");
		
	comment = (String) comments.get(0);
	result = (Long) results.get(0);

	if(result == 1){
		label = "pos";
	} else {
		label = "neg";
	}
	get(Fields.Out, "comment").setValue(output, comment);
	get(Fields.Out, "label").setValue(output, label);
		
	putRow(data.outputRowMeta, output);

    return true;
}</class_source>
